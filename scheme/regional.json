{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Schema for Regional Data",
    "type": "object",
    "properties": {
      "data": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "regionid": {
              "type": "number"
            },
            "dnoregion": {
              "type": "string"
            },
            "shortname": {
              "type": "string"
            },
            "postcode": {
              "type": "string"
            },
            "data": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "from": {
                    "type": "string"
                  },
                  "to": {
                    "type": "string"
                  },
                  "intensity": {
                    "type": "object",
                    "properties": {
                      "forecast": {
                        "type": "number"
                      },
                      "index": {
                        "type": "string"
                      }
                    },
                    "required": [
                      "forecast",
                      "index"
                    ]
                  },
                  "generationmix": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "fuel": {
                          "type": "string"
                        },
                        "perc": {
                          "type": "number"
                        }
                      },
                      "required": [
                        "fuel",
                        "perc"
                      ]
                    }
                  }
                },
                "required": [
                  "from",
                  "to",
                  "intensity",
                  "generationmix"
                ]
              }
            }
          },
          "required": [
            "regionid",
            "dnoregion",
            "shortname",
            "postcode",
            "data"
          ]
        }
      }
    },
    "required": [
      "data"
    ]
  }